<launch>
  <node name="cartographer_node" pkg="cartographer_ros" type="cartographer_node" args="
    -configuration_directory $(find happy_robo)/lua
    -configuration_basename my_robot.lua"
    output="screen">
    <remap from="/scan" to="/merged/scan"/>
    <!-- <remap from="/imu" to="/imu/data" /> -->
    <remap from="/imu" to="/wit/imu" />
    <!-- <remap from="/odom" to="/odom" /> -->
    <remap from="/odom" to="/odom/raw" />
  </node>
  <node name="cartographer_occupancy_grid_node" pkg="cartographer_ros" type="cartographer_occupancy_grid_node" args="-resolution 0.05" output="screen" />
</launch>