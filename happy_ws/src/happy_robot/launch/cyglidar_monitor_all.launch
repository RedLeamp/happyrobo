<launch>

  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" output="screen" />
    <param name="robot_description" command="$(find xacro)/xacro $(find happy_robo)/urdf/real.urdf.xacro" />

    <node pkg="tf" type="static_transform_publisher" name="map_base_link" args="0.000 0.000 0.000 0.000 0.00 0.00 map base_footprint 0" />

<arg name="run_mode" value="0" doc="version type [0(2D), 1(3D), 2(2D/3D)]"/>
  <include file="$(find cyglidar_d2_ros1)/launch/cyglidar_monitor.launch">
    <arg name="number" value="0" />
    <arg name="port" value="0" />
    <arg name="run_mode" value="$(arg run_mode)" doc="version type [0(2D), 1(3D), 2(2D/3D)]"/>
    <!-- <arg name="laser_tf" default="0.23 0.17 0.34 0 0 0 map laser_frame_0 15" /> -->
  </include>
  <include file="$(find cyglidar_d2_ros1)/launch/cyglidar_monitor.launch">
    <arg name="number" value="1"/>
    <arg name="port" value="1" />
    <arg name="run_mode" value="$(arg run_mode)" doc="version type [0(2D), 1(3D), 2(2D/3D)]"/>
    <!-- <arg name="laser_tf" default="0.23 -0.17 0.34 0 0 0 map laser_frame_1 15" /> -->
  </include>
  <include file="$(find cyglidar_d2_ros1)/launch/cyglidar_monitor.launch">
    <arg name="number" value="2" />
    <arg name="port" value="2" />
    <arg name="run_mode" value="$(arg run_mode)" doc="version type [0(2D), 1(3D), 2(2D/3D)]"/>
    <!-- <arg name="laser_tf" default="-0.23 0.17 0.34 3.14159 0 0 map laser_frame_2 15" /> -->
  </include>
  <include file="$(find cyglidar_d2_ros1)/launch/cyglidar_monitor.launch">
    <arg name="number" value="3" />
    <arg name="port" value="3" />
    <arg name="run_mode" value="$(arg run_mode)" doc="version type [0(2D), 1(3D), 2(2D/3D)]"/>
    <!-- <arg name="laser_tf" default="-0.23 -0.17 0.34 3.14159 0 0 map laser_frame_3 15" /> -->
  </include>
    <include file="$(find cyglidar_d2_ros1)/launch/cyglidar_monitor.launch">
    <arg name="number" value="4" />
    <arg name="port" value="4" />
    <arg name="run_mode" value="$(arg run_mode)" doc="version type [0(2D), 1(3D), 2(2D/3D)]"/>
    <!-- <arg name="laser_tf" default="0.23 0.17 0.34 1.17089 0 0 map laser_frame_4 15" /> -->
  </include>
    <include file="$(find cyglidar_d2_ros1)/launch/cyglidar_monitor.launch">
    <arg name="number" value="5" />
    <arg name="port" value="5" />
    <arg name="run_mode" value="$(arg run_mode)" doc="version type [0(2D), 1(3D), 2(2D/3D)]"/>
    <!-- <arg name="laser_tf" default="0.23 -0.17 0.34 -1.17089 0 0 map laser_frame_5 15" /> -->
  </include>
    <include file="$(find cyglidar_d2_ros1)/launch/cyglidar_monitor.launch">
    <arg name="number" value="6" />
    <arg name="port" value="6" />
    <arg name="run_mode" value="$(arg run_mode)" doc="version type [0(2D), 1(3D), 2(2D/3D)]"/>
    <!-- <arg name="laser_tf" default="-0.23 0.17 0.34 1.17089 0 0 map laser_frame_6 15" /> -->
  </include>
  <include file="$(find cyglidar_d2_ros1)/launch/cyglidar_monitor.launch">
    <arg name="number" value="7" />
    <arg name="port" value="7" />
    <arg name="run_mode" value="$(arg run_mode)" doc="version type [0(2D), 1(3D), 2(2D/3D)]"/>
    <!-- <arg name="laser_tf" default="-0.23 -0.17 0.34 -1.17089 0 0 map laser_frame_7 15" /> -->
  </include>
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find cyglidar_d2_ros1)/rviz/cyglidar_config_all.rviz"/>
</launch>
